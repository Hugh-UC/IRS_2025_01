<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="tm16">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import tm16 urdf file -->
    <xacro:include filename="$(find tm_description)/xacro/tm16.urdf.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="tm16.ros2_control.xacro" />


    <xacro:tm16_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
